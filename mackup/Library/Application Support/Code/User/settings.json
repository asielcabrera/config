{
  "editor.accessibilitySupport": "off",
  "editor.codeActionsOnSave": {
    "source.fixAll": false
  },
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.fontFamily": "SFMono Nerd Font",
  "editor.fontLigatures": true,
  "editor.fontSize": 18,
  "editor.formatOnSave": true,
  "editor.inlineSuggest.enabled": true,
  "git.autofetch": true,
  "redhat.telemetry.enabled": true,
  "terminal.external.osxExec": "Alarcitty.app",
  "terminal.integrated.env.osx": {
    "FIG_NEW_SESSION": "1"
  },
  "terminal.integrated.fontSize": 16,
  "vim.easymotion": true,
  "vim.enableNeovim": true,
  "vim.leader": " ",
  "vim.neovimPath": "/opt/homebrew/bin/nvim",
  "vim.statusBarColorControl": true,
  "vim.useSystemClipboard": true,
  "window.nativeFullScreen": false,
  "workbench.colorCustomizations": {
    "statusBar.background": "#005f5f",
    "statusBar.noFolderBackground": "#005f5f",
    "statusBar.debuggingBackground": "#005f5f"
  },
  "workbench.colorTheme": "Catppuccin Mocha",
  "workbench.iconTheme": "vscode-icons",
  "gitlens.blame.toggleMode": "window",
  "gitlens.blame.highlight.enabled": false,
  "transparent-win.transparency": 0.8,
  "window.titleBarStyle": "native",
  "workbench.layoutControl.enabled": false,
  "customizeUI.activityBar": "bottom",
  "customizeUI.activityBarHideSettings": true,
  "customizeUI.statusBarPosition": "under-panel",
  "customizeUI.titleBar": "frameless"
}
